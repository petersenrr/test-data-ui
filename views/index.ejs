<% page = { 'title' : 'PAGE_TITLE', 'key': 'INDEX_PAGE_NAME'} %>
<% assets = {
  'css' : ['/css/bootstrap.min.css'],
  'js' : [{'defer': '/js/sample.js'}, '/modules/sample/sample.js'],
  'html' : ['/elements/bundles/test.html']
} %>
<link href="/css/test.css" rel="stylesheet" type="text/css">
<script>
  s = {};
  _satellite = {};
</script>
<div id="main" class="experiments">
  <div class="no-layout">
    <h1 id="opening" class="museo,serif;">Create Family</h1>
    <hr>
    <section class="basic">
        <div class="containingBox">

          <div class="template">
              <label for="dropTemplate" id="templateLabel"><h6>Template:</h6></label>
              <select id="dropTemplate">
                  <option value="Basic" selected>Basic Family</option>
                  <option value="Indicator.DataQuality">Data Quality Family</option>
                  <option value="Default">Default Family</option>
                  <option value="Extended">Extended Family</option>
                  <option value="Generational">Generation Family</option>
                  <option value="Indicators">Indicator Family</option>
                  <option value="Living">Living Family</option>
                  <option value="Localized">Localized Family</option>
                  <option value="Multiple.Husbands"">Multi Husband Family</option>
                  <option value="Multiple.Parents">Multi Parent Family</option>
                  <option value="Multiple.Wives">Multi Wife Family</option>
                  <option value="Private">Private Family</option>
                  <option value="publicPrivate">Public Private Family</option>
                  <option value="Indicator.ResearchSuggestion"">Research Suggestion Family</option>
                  <option value="Single.Father">Single Father Family</option>
                  <option value="Single.Mother">Single Mother Family</option>
                  <option value="Temple">Temple Family</option>
              </select>
          </div>


          <label for="sessionId" id="sessionLabel"><h6>SessionID:</h6></label>
          <input type="text" id="sessionId">

          <div id="childrenSection">
            <label for="children" ><h6>Number of children:</h6></label>
            <input type="text" id="children" placeholder="6">
          </div>

          <div id="marriageSection">
            <div class="dropdown">
                <label for="drop" ><h6>Marriage event:</h6></label>
                <select id="drop">
                    <option value="true">Yes</option>
                    <option value="false" selected>No</option>
                </select>
            </div>
          </div>

          <div id="templeSection">
            <label for="templeReady" ><h6>Temple Ready:</h6></label>
            <input type="text" id="templeReady" placeholder="false">
          </div>

          <div id="gen">
            <label for="generations" ><h6>Generations (Enter 0 for none):</h6></label>
            <input type="text" id="generations" placeholder="3">
          </div>


          <div id="dateSection">
            <label for="date" ><h6>Date:</h6></label>
            <input type="text" id="date" placeholder="MM/DD/YYYY">
          </div>

          <div id="familyNameSection">
            <label for="familyName" ><h6>Family Name:</h6></label>
            <input type="text" id="familyName" placeholder="Smith">
          </div>

          <div id="husbandsSection">
            <label for="husbands" ><h6>Husbands:</h6></label>
            <input type="text" id="husbands" placeholder="2">
          </div>

          <div id="wivesSection">
            <label for="Wives" ><h6>Wives:</h6></label>
            <input type="text" id="wives" placeholder="3">
          </div>


          <div id="localeSection">
            <div class="template">
              <label for="dropLocale" ><h6>Locale:</h6></label>
              <select id="dropLocale">
                  <option value="sq">Albanian</option>
                  <option value="hy">Armenian</option>
                  <option value="bg">Bulgarian</option>
                  <option value="zh">Chinese</option>
                  <option value="hr">Croatian</option>
                  <option value="cs">Czech</option>
                  <option value="da">Danish</option>
                  <option value="nl">Dutch</option>
                  <option value="en" selected>English</option>
                  <option value="et">Estonian</option>
                  <option value="fj">Fijian</option>
                  <option value="fi">Finnish</option>
                  <option value="fr">French</option>
                  <option value="de">German</option>
                  <option value="el">Greek</option>
                  <option value="ht">Haitian</option>
                  <option value="hu">Hungarian</option>
                  <option value="is">Icelandic</option>
                  <option value="id">Indonesian</option>
                  <option value="it">Italian</option>
                  <option value="ja">Japanese</option>
                  <option value="km">Khmer</option>
                  <option value="ko">Korean</option>
                  <option value="lv">Latvian</option>
                  <option value="lt">Lithuanian</option>
                  <option value="mk">Macedonian</option>
                  <option value="ms">Malay</option>
                  <option value="mn">Mongolian</option>
                  <option value="no">Norwegian</option>
                  <option value="pl">Polish</option>
                  <option value="pt">Portuguese</option>
                  <option value="ro">Romanian</option>
                  <option value="ru">Russian</option>
                  <option value="sm">Samoan</option>
                  <option value="sr">Serbian</option>
                  <option value="sk">Slovak</option>
                  <option value="sl">Slovenian</option>
                  <option value="es">Spanish</option>
                  <option value="sv">Swedish</option>
                  <option value="th">Thai</option>
                  <option value="to">Tongan</option>
                  <option value="uk">Ukrainian</option>
                  <option value="vi">Vietnamese</option>
              </select>
            </div>
          </div>

          <div id="parentsSection">
            <label for="parents" ><h6>Parents:</h6></label>
            <input type="text" id="parents" placeholder="1">
          </div>

          <div id="placeSection">
            <label for="place" ><h6>Place:</h6></label>
            <input type="text" id="place" placeholder="Orem, UT">
          </div>

          <div id="livingSection">
            <div class="dropdown">
              <label for="dropLiving" ><h6>Living:</h6></label>
              <select id="dropLiving">
                  <option value="true">Yes</option>
                  <option value="false" selected>No</option>
              </select>
            </div>
          </div>

          <div id="qualitySection">
            <div class="dropdown">
                <label for="dropDataQuality" ><h6>Data Quality:</h6></label>
                <select id="dropDataQuality">
                    <option value="true">Yes</option>
                    <option value="false" selected>No</option>
                </select>
            </div>
          </div>

          <div id="researchSection">
            <div class="dropdown">
                <label for="dropResearch" ><h6>Research Suggestions:</h6></label>
                <select id="dropResearch">
                    <option value="true">Yes</option>
                    <option value="false" selected>No</option>
                </select>
            </div>
          </div>

          <div id="privateSection">
            <div class="dropdown">
                <label for="dropPrivate" ><h6>Private:</h6></label>
                <select id="dropPrivate">
                    <option value="true">Yes</option>
                    <option value="false" selected>No</option>
                </select>
            </div>
          </div>

        
          <hr>
          <a href="#" id="createButton"  onclick="createFamily()" class="btn btn-default">Create Family</a>
        </div>
    </section>
  </div>
</div>
